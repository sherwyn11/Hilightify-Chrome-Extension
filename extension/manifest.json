{
	"manifest_version": 2,
	"name": "Spotify Custom Playlist",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js" : [
				"content.js"
			]
		}
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"1": "imgs/spotify.png"
		},
		"default_title": "Spotify Custom Playlist",
		"default_popup": "popup.html" 
	}
}